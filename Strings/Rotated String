
// roated string exists in double of same str   as vishesh - isheshv ... exists in v'isheshv'ishesh
public:
    bool rotateString(string s, string goal) {
        if(s.length() != goal.length())return false;

        string temp = s + s ;
        if(temp.find(goal) !=string::npos)return true;
        else return false;
    }
